{
  "$id": "common.MetricDataset.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "data": {
      "description": "Data.\n Each value index corresponds with the same timestamp index of the Metric.",
      "items": {
        "anyOf": [
          {
            "maximum": 3.4028234663852886e+38,
            "minimum": -3.4028234663852886e+38,
            "type": "number"
          },
          {
            "enum": [
              "Infinity",
              "-Infinity",
              "NaN"
            ],
            "type": "string"
          }
        ]
      },
      "type": "array"
    },
    "label": {
      "description": "Label.",
      "type": "string"
    }
  },
  "required": [
    "label"
  ],
  "title": "Metric Dataset",
  "type": "object"
}
