{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "setup.schema.json",
  "title": "ChirpStack Setup Configuration",
  "description": "Schema for ChirpStack provisioning setup data that defines tenants, applications, gateways, device profiles, and users in a hierarchical structure",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "device_profile_templates": {
      "type": "array",
      "description": "Device profile templates that can be referenced by device profiles",
      "items": {
        "$ref": "api.DeviceProfileTemplate.jsonschema.strict.json"
      }
    },
    "users": {
      "type": "array",
      "description": "Global users that can be assigned to tenants",
      "items": {
        "$ref": "api.User.jsonschema.strict.json"
      }
    },
    "tenants": {
      "type": "array",
      "description": "Tenants with their associated applications, gateways, device profiles, and users",
      "items": {
        "$ref": "#/$defs/TenantWithChildren"
      }
    }
  },
  "$defs": {
    "TenantWithChildren": {
      "type": "object",
      "description": "Tenant object with nested children (gateways, applications, device_profiles, users). Note: Properties are referenced individually because the base Tenant schema has additionalProperties:false, which cannot be overridden in allOf.",
      "properties": {
        "id": {
          "$ref": "api.Tenant.jsonschema.strict.json#/properties/id"
        },
        "name": {
          "$ref": "api.Tenant.jsonschema.strict.json#/properties/name"
        },
        "description": {
          "$ref": "api.Tenant.jsonschema.strict.json#/properties/description"
        },
        "canHaveGateways": {
          "$ref": "api.Tenant.jsonschema.strict.json#/properties/canHaveGateways"
        },
        "maxGatewayCount": {
          "$ref": "api.Tenant.jsonschema.strict.json#/properties/maxGatewayCount"
        },
        "maxDeviceCount": {
          "$ref": "api.Tenant.jsonschema.strict.json#/properties/maxDeviceCount"
        },
        "privateGatewaysUp": {
          "$ref": "api.Tenant.jsonschema.strict.json#/properties/privateGatewaysUp"
        },
        "privateGatewaysDown": {
          "$ref": "api.Tenant.jsonschema.strict.json#/properties/privateGatewaysDown"
        },
        "tags": {
          "$ref": "api.Tenant.jsonschema.strict.json#/properties/tags"
        },
        "gateways": {
          "type": "array",
          "description": "Gateways associated with this tenant",
          "items": {
            "$ref": "api.Gateway.jsonschema.strict.json"
          }
        },
        "applications": {
          "type": "array",
          "description": "Applications associated with this tenant",
          "items": {
            "$ref": "#/$defs/ApplicationWithIntegrations"
          }
        },
        "device_profiles": {
          "type": "array",
          "description": "Device profiles associated with this tenant",
          "items": {
            "$ref": "api.DeviceProfile.jsonschema.strict.json"
          }
        },
        "users": {
          "type": "array",
          "description": "Users associated with this tenant",
          "items": {
            "$ref": "api.User.jsonschema.strict.json"
          }
        }
      },
      "required": [
        "id",
        "name",
        "description",
        "canHaveGateways",
        "maxGatewayCount",
        "maxDeviceCount",
        "privateGatewaysUp",
        "privateGatewaysDown"
      ],
      "additionalProperties": false
    },
    "ApplicationWithIntegrations": {
      "type": "object",
      "description": "Application object with nested integrations and multicast_groups. Note: Properties are referenced individually because the base Application schema has additionalProperties:false, which cannot be overridden in allOf.",
      "properties": {
        "id": {
          "$ref": "api.Application.jsonschema.strict.json#/properties/id"
        },
        "name": {
          "$ref": "api.Application.jsonschema.strict.json#/properties/name"
        },
        "description": {
          "$ref": "api.Application.jsonschema.strict.json#/properties/description"
        },
        "tenantId": {
          "$ref": "api.Application.jsonschema.strict.json#/properties/tenantId"
        },
        "tags": {
          "$ref": "api.Application.jsonschema.strict.json#/properties/tags"
        },
        "integrations": {
          "type": "object",
          "description": "Integrations associated with this application. Keys are integration types.",
          "additionalProperties": false,
          "properties": {
            "InfluxDbIntegration": {
              "$ref": "api.InfluxDbIntegration.jsonschema.strict.json"
            },
            "HttpIntegration": {
              "$ref": "api.HttpIntegration.jsonschema.strict.json"
            },
            "AwsSnsIntegration": {
              "$ref": "api.AwsSnsIntegration.jsonschema.strict.json"
            },
            "AzureServiceBusIntegration": {
              "$ref": "api.AzureServiceBusIntegration.jsonschema.strict.json"
            },
            "BlynkIntegration": {
              "$ref": "api.BlynkIntegration.jsonschema.strict.json"
            },
            "GcpPubSubIntegration": {
              "$ref": "api.GcpPubSubIntegration.jsonschema.strict.json"
            },
            "IftttIntegration": {
              "$ref": "api.IftttIntegration.jsonschema.strict.json"
            },
            "MyDevicesIntegration": {
              "$ref": "api.MyDevicesIntegration.jsonschema.strict.json"
            },
            "PilotThingsIntegration": {
              "$ref": "api.PilotThingsIntegration.jsonschema.strict.json"
            },
            "ThingsBoardIntegration": {
              "$ref": "api.ThingsBoardIntegration.jsonschema.strict.json"
            }
          }
        },
        "multicast_groups": {
          "type": "array",
          "description": "Multicast groups associated with this application",
          "items": {
            "$ref": "api.MulticastGroup.jsonschema.strict.json"
          }
        }
      },
      "required": [
        "id",
        "name",
        "description",
        "tenantId"
      ],
      "additionalProperties": false
    }
  }
}
